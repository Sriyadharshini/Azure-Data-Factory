{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "handsonadftest"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDatabaseconnectivity_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseconnectivity'"
		},
		"Is_onprem_fileservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Is_onprem_fileservice'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://handsonstorageadf.dfs.core.windows.net/"
		},
		"AzureSqlDatabaseconnectivity_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "handsonsri.database.windows.net"
		},
		"AzureSqlDatabaseconnectivity_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "handsondatabasesri"
		},
		"AzureSqlDatabaseconnectivity_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsri"
		},
		"HttpServerforgit_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"Is_onprem_fileservice_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\desktop-df8ial0\\onpremdata"
		},
		"Is_onprem_fileservice_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "desktop-df8ial0\\admin"
		},
		"trigger data movement_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a803c776-63e6-47d9-889b-21fd0ff6dfed/resourceGroups/rghandsoneastus/providers/Microsoft.Storage/storageAccounts/handsonstorageadf"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "handsonadftest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabaseconnectivity_password": {
						"value": "[parameters('AzureSqlDatabaseconnectivity_password')]"
					},
					"Is_onprem_fileservice_password": {
						"value": "[parameters('Is_onprem_fileservice_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureSqlDatabaseconnectivity_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseconnectivity_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseconnectivity_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseconnectivity_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseconnectivity_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseconnectivity_properties_typeProperties_userName')]"
					},
					"HttpServerforgit_properties_typeProperties_url": {
						"value": "[parameters('HttpServerforgit_properties_typeProperties_url')]"
					},
					"Is_onprem_fileservice_properties_typeProperties_host": {
						"value": "[parameters('Is_onprem_fileservice_properties_typeProperties_host')]"
					},
					"Is_onprem_fileservice_properties_typeProperties_userId": {
						"value": "[parameters('Is_onprem_fileservice_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "handsonadftest_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger data movement_properties_typeProperties_scope": {
						"value": "[parameters('trigger data movement_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'handsonadftest_ArmTemplate_0')]"
			]
		}
	]
}